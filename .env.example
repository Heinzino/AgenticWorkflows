# Google API credentials
# Get these from Google Cloud Console: https://console.cloud.google.com/
GOOGLE_SHEETS_CREDENTIALS_PATH=credentials.json
GOOGLE_TOKEN_PATH=token.json

# Modal API token (for webhooks)
# Get from: https://modal.com/settings/tokens
MODAL_TOKEN_ID=
MODAL_TOKEN_SECRET=

# Slack webhook (optional, for notifications)
SLACK_WEBHOOK_URL=

# Email configuration (if using email tools)
SMTP_HOST=
SMTP_PORT=587
SMTP_USER=
SMTP_PASSWORD=
EMAIL_FROM=

# API keys for various services (add as needed)
OPENAI_API_KEY=
ANTHROPIC_API_KEY=

# PandaDoc API
# Get from: https://app.pandadoc.com/a/#/settings/integrations/api
PANDADOC_API_KEY=your_api_key_here
PANDADOC_TEMPLATE_ID=your_template_id_here

# Other service credentials
# Add your own as needed
